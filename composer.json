{
    "name": "totaa/totaa-poster",
    "description": "Poster Manager system",
    "keywords": [
        "totaa",
        "totaa-poster"
    ],
    "homepage": "https://github.com/totaa/totaa-poster",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Tho Thanh TÃ¢m",
            "email": "thanhtamtotaa@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Totaa\\TotaaPoster\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Totaa\\TotaaPoster\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Totaa\\TotaaPoster\\TotaaPosterServiceProvider"
            ],
            "aliases": {
                "TotaaPoster": "Totaa\\TotaaPoster\\TotaaPosterFacade"
            }
        }
    }
}
